{
  "name": "ESGI GRADES 2.0",
  "description": "Calc les blocs de comp√©tences de l'ESGI",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.myges.fr/student/marks/*"
      ],
      "js": [
        "./static/js/content.js"
      ],
      "all_frames": false,
      "run_at": "document_end"
    }
  ]
}